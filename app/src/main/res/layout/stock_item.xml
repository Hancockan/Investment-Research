<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="10dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp">

    <TextView
        android:id="@+id/ticker_symbol"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/last_trade_price"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#143b37"
        android:text="GOOG"/>

    <TextView
        android:id="@+id/company_name"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_below="@+id/ticker_symbol"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="italic"
        android:text="Alphabet, Inc."/>

    <TextView
        android:id="@+id/last_trade_price"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#143b37"
        android:text="831.33"/>

    <TextView
        android:id="@+id/day_high"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_below="@+id/company_name"
        android:layout_alignParentRight="true"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#000000"
        android:text="834.67"/>

    <TextView
        android:id="@+id/day_high_label"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_below="@id/company_name"
        android:layout_toLeftOf="@id/day_high"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#37a500"
        android:text="Day High: "/>

    <TextView
        android:id="@+id/day_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/day_high"
        android:layout_alignParentRight="true"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#000000"
        android:text="545.23"/>

    <TextView
        android:id="@+id/day_low_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/day_high_label"
        android:layout_toLeftOf="@id/day_low"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#df0000"
        android:text="Day Low: "/>

    <TextView
        android:id="@+id/year_high"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_below="@+id/day_low"
        android:layout_alignParentRight="true"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#000000"
        android:text="967.67"/>

    <TextView
        android:id="@+id/year_high_label"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_toLeftOf="@id/year_high"
        android:layout_below="@id/day_low"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#37a500"
        android:text="Year High: "/>

    <TextView
        android:id="@+id/year_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/year_high"
        android:layout_alignParentRight="true"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#000000"
        android:text="345.23"/>

    <TextView
        android:id="@+id/year_low_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/year_high_label"
        android:layout_toLeftOf="@id/year_low"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#df0000"
        android:text="Year Low: "/>

    <TextView
        android:id="@+id/daily_volume"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/year_low"
        android:layout_alignParentRight="true"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#000000"
        android:text="2,098,543,523"/>

    <TextView
        android:id="@+id/daily_volume_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/daily_volume"
        android:layout_below="@+id/year_low_label"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#000000"
        android:text="Daily Volume: "/>

    <TextView
        android:id="@+id/daily_change_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/daily_volume_label"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#000000"
        android:text="Daily Change: "/>

    <TextView
        android:id="@+id/daily_change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@id/daily_volume"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#000000"
        android:text="NULL"/>

</RelativeLayout>
